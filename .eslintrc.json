{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "globals": {
        "React": true,
        "afterEach": true,
        "beforeEach": true,
        "describe": true,
        "expect": true,
        "test": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "semi": [2, "never"],
        "dot-location": [2, "object"],
        "no-console": [2, { "allow": ["error"] }],
        "no-multi-assign": 0,
        "comma-dangle": [2, { "arrays": "never" }],
        "import/extensions": 0,
        "func-names": 0,
        "space-before-function-paren": [2, "never"],
        "react/forbid-prop-types": [2, { "forbid": ["any"] }],
        "react/require-default-props": 0,
        "react/jsx-no-bind": 0,
        "no-plusplus": 0,
        "arrow-parens": 0,
        "no-param-reassign": [2, { "props": false }],
        "import/no-extraneous-dependencies": [2, {
            "devDependencies": true, "optionalDependencies": false, "peerDependencies": false
        }]
    }
}
